
@{
    ViewBag.Title = "SellerOrder";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="memberSeller_order_status container mt-5">
    <div class="memberSeller row">
        <ul class="memberSeller_state d-flex justify-content-start mt-5">
            <a class="me-4" href="EditSpace">
                <li class="p-2">處理中</li>
            </a>
            <a class="me-4" href="Completed">
                <li class="p-2">歷史訂單</li>
            </a>
        </ul>
    </div>
</div>

<div class="memberSeller_container container mt-2">
    <div class=" row d-flex border-top  justify-content-end ">
        <div class="col-12 col-lg-4 mt-5 ">
            <p>訂單編號：<span>2109020001</span></p>
        </div>
        <div class="col-12 col-lg-8 mt-5 text-start text-xl-end">
            訂單狀態：<span class="me-2 CurrentState">尚未過取消期限</span><span class="me-2 d-lg-inline d-none notCurrentState">已過取消期限</span><span class=" notCurrentState d-lg-inline d-none">正在使用中</span>
        </div>
    </div>
    <div class="row  ">
        <div class="col memberSeller_completed_pic col-12 col-lg-6 col-xl-3 ">
            <div>
                <img class="p-2 mt-2" src="https://picsum.photos/300/200/?random=1">
            </div>

        </div>
        <div class="memberSeller_completed_txt_title col-12 col-md-12   col-xl-6  mt-3 mb-5">
            <h6>場地名稱:Good Space</h6>
            <p>訂購人:<span>王曉明</span></p>
            <p>聯絡人:<span>王大明</span></p>
            <p>聯絡電話:<span>0912345678</span></p>
            <p>租借時間:<span class="me-2">2021年10月01日</span><span class="me-1">星期二</span><span class="me-1 ">09:00</span></p>
            <!-- <p><span class="textms  d-inline d-md-none d-lg-none">星期二</span><span class="ms-2 d-inline d-md-none d-lg-none" >09:00</span></p></p> -->
            <p>租還時間:<span class="me-2">2021年10月01日</span><span class="me-1">星期二</span><span class="me-1">13:00</span></p>
            <!-- <p><span class="textms  d-inline d-md-none d-lg-none">星期二</span><span class="ms-2 d-inline d-md-none d-lg-none" >13:00</span></p></p> -->
            <div class="row">
                <div class="price col-12 d-flex   d-md-none d-lg-flex d-xl-none justify-content-end align-text-bottom  text-end">
                    <p class="fs18 ZoneRadarB">
                        訂單價格：
                        <span class=" memberSeller_completed_txt_price ">TWD1,000元</span>
                    </p>
                </div>
            </div>
        </div>
        <div class="col-12 d-none d-md-flex d-lg-none d-xl-flex  col-lg-6 col-xl-12  justify-content-end align-text-bottom  mt-auto text-end">
            <p class="fs18 ZoneRadarB">
                訂單價格：
                <span class=" memberSeller_completed_txt_price ">TWD1,000元</span>
            </p>
        </div>
    </div>
</div>

