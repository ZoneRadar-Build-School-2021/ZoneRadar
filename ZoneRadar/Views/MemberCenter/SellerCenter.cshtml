                        <button class="EditSpaceBtn me-1">
                            編輯空間
                        </button>
                    </a>
                    <button class="DeleteSpaceBtn" type="button" data-bs-toggle="modal" data-bs-target="#deleteModal">
                        刪除
                    </button>
                </div>
            </div>
            <!-- 按下刪除跳出的Ｍodal -->
            <!-- 桌機 平板 手機橫 -->
            <!-- Modal -->
            <div class="modal fade " id="deleteModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
                <div class="modal-dialog">
                    <div class="modal-content ">
                        <button type="button" class="btn-close CloseMbtn ms-auto" data-bs-dismiss="modal" aria-label="Close"></button>
                        <h5 class="text-center mt-3 mb-4" id="exampleModalLabel">刪除場地</h5>
                        <div class="modalbody ms-auto me-auto">
                            <p class=" text-center  text-md-start">您是否確定將此場地刪除？</p>
                            <div class="form-check ms-auto me-auto w-75">
                                <input class="form-check-input " type="checkbox" value="" id="flexCheckDefault">
                                <label class="form-check-label " for="flexCheckDefault">是的，我清楚刪除此場地相關的所有列表訊息</label>

                            </div>
                        </div>
                        <div class="row justify-content-center mt-5 mb-2 ">
                            <button type="button" class="w-75 col-8 btn deleteBtnM " data-bs-dismiss="modal">刪除</button>
                        </div>
                        <div class="row justify-content-center mb-4">
                            <button type="button" class="w-75 col-8 text-center btn cancelBtnM " data-bs-dismiss="modal" aria-label="Close">取消</button>
                        </div>
                    </div>
                </div>
            </div>

        </div>
