
@{
    ViewBag.Title = "SellerCenter";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="mymain">
    <div class="container">
        <div class="row row-cols-2  d-lg-flex mb-1 ">
            <div class="col col-lg-6 ">
                <h2 class="fs-4">場地管理</h2>
            </div>
            <div class="col col-lg-6  justify-content-lg-end text-end ">
                <a href="AddSpace">
                    <button class="AddSpaceBtn ">
                        增加空間
                    </button>
                </a>
            </div>
        </div>
        <div class="container">
            <!-- 手機橫 平板直 -->
            <div class="row SpaceCardGroup d-md-flex d-none d-lg-none d-xl-none justify-content-between">
                <div class="col SpaceCardimg">
                    <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRrxaDGTL9uO8NrEQSmy5vcO1b9wzR9vfmnfQ3cG8Bqk2G_Ze4Myu74UO-02RCXFiA3DbY&usqp=CAU">
                </div>
                <div class="col-md-8 mt-auto mb-auto">
                    <div class="row">
                        <div class="col-md-12 SpaceCardBody ">
                            <h5>Good Space Meeting</h5>
                            <i class="far fa-star"></i>
                            <i class="far fa-star"></i>
                            <i class="far fa-star"></i>
                            <i class="far fa-star"></i>
                            <i class="far fa-star"></i>
                            <!-- 內容 地址 提供的服務列3項 加取消政策 -->
                            <p>台北市內湖區文德路,提供車位,Wifi,取消政策</p>
                        </div>
                        <div class="col-md-12">
                            <a href="AddSpace">
                                <button class="EditSpaceBtn me-1 ">
                                    編輯空間
                                </button>
                            </a>
                            <button class="DeleteSpaceBtn" type="button" data-bs-toggle="modal" data-bs-target="#deleteModal">
                                刪除
                            </button>
                        </div>
                    </div>
                </div>
            </div>
            <!-- 桌機 手機直 平板橫 -->
            <div class="row SpaceCardGroup  d-lg-flex  d-md-none justify-content-lg-between">
                <div class="col SpaceCardimg col-12 col-lg-2 col-xl-1 ">
                    <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRrxaDGTL9uO8NrEQSmy5vcO1b9wzR9vfmnfQ3cG8Bqk2G_Ze4Myu74UO-02RCXFiA3DbY&usqp=CAU">
                </div>
                <div class="col SpaceCardBody col-12  col-xl-6  col-lg-6 ">
                    <h5>Good Space Meeting</h5>

                    <!-- 內容 地址 提供的服務列3項 加取消政策 -->
                    <p>台北市內湖區文德路,提供車位,Wifi,取消政策</p>
                </div>
                <div class="col-12  d-flex  justify-content-start d-md-none d-lg-none mb-2">
